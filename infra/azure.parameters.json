{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "bot${{RESOURCE_SUFFIX}}"
    },
    "botAadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "botAadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "hello-ai-bot"
    },
    "azOpenAiKey": {
      "value": "${{SECRET_AZURE_OPENAI_API_KEY}}"
    },
    "azOpenAiEndpoint": {
      "value": "${{SECRET_AZURE_OPENAI_ENDPOINT}}"
    },
    "azOpenAiDeployment": {
      "value": "${{SECRET_AZURE_OPENAI_DEPLOYMENT}}"
    },
    "aadTenantId": {
      "value": "${{AAD_APP_TENANT_ID}}"
    },
    "aadAppClientId": {
      "value": "${{AAD_APP_CLIENT_ID}}"
    },
    "aadAppOauthAuthority": {
      "value": "${{AAD_APP_OAUTH_AUTHORITY}}"
    },
    "aadAppClientSecret": {
      "value": "${{SECRET_AAD_APP_CLIENT_SECRET}}"
    },
    "azContentSafetyEndpoint": {
      "value": "${{SECRET_AZURE_CONTENT_SAFETY_ENDPOINT}}"
    },
    "azContentSafetyKey": {
      "value": "${{SECRET_AZURE_CONTENT_SAFETY_KEY}}"
    }
  }
}